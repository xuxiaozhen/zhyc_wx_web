<!--pages/number/number.wxml-->

<view class="container" >
  <view class="title">党员档案</view>
  <view class="personal">个人电子名片</view>
  <view class="personal-information">
    <image class="photo"></image>
    <view class="message">
      <view class="row">
        <text>姓名：{{numberMsg.name}}</text>
        <text>座机：{{numberMsg.phone}}</text>
      </view>
      <view class="row">
        <text>部门：{{numberMsg.deptname}}</text>
        <text>生日：{{numberMsg.birthdate}}</text>
      </view>
      
      <!-- <text>部门：</text>
      <text>电话：</text> -->
    </view>
  </view>

  

</view>